@page "/test"

<h3>@count</h3>

<button @onclick="@ClickButton">Click</button>

<table>
    @for (int i = 0; i < 3; i++){
        <tr>
            @for (int j = 0; j < 3; j++)
            {
                
                <th>@j</th>
            }
        </tr>
    }
</table>


@code {
    private int count = 0;

    private void ClickButton()
    {
        count++;
    }
}
